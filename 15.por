programa {
  funcao inicio() {
    real litro, quilometragem, media_consumo
    escreva("Quantos litros? ")
    leia(litro)
    escreva("Quantos quilometros percorreu? ")
    leia(quilometragem)
    media_consumo = quilometragem / litro
    escreva("Seu veículo tem a eficiência de ", media_consumo, " KM a cada 1 litro" )
    
    
  }
}
